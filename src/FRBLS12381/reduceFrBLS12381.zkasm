;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; POST: The result is in the range [0,BLS12_381_R)
;;
;; reduceFrBLS12381:
;;             in: A ∈ [0, 2²⁵⁶-1]
;;             out: A = A (mod BLS12_381_R) ∈ Fr
;;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

reduceFrBLS12381:
        ; A·1 + 0 = op (mod BLS12_381_R)
        1 => B
        0 => C
        %BLS12_381_R => D
        ${A % D} => E           :ARITH_MOD
        E => A                  :RETURN